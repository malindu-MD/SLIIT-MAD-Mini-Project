                                android:layout_height="wrap_content"
                                android:text="@string/price"
                                android:layout_toRightOf="@id/s1"
                                android:layout_below="@id/s3"
                                android:layout_marginTop="10dp"
                                android:layout_marginLeft="20dp"
                                android:textSize="18sp"
                                android:textStyle="bold"
                                android:id="@+id/s4"
                                android:textColor="#000000"/>


                            <Button
                                android:id="@+id/s5"
                                android:layout_width="74dp"
                                android:layout_height="38dp"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginTop="30dp"

                                android:layout_marginRight="14dp"
                                android:layout_marginBottom="12dp"
                                android:backgroundTint="@color/red"
                                android:text="Delete"
                                android:textSize="10sp"
                                android:textStyle="bold" />

                            <Button
                                android:id="@+id/s6"
                                android:layout_width="63dp"
                                android:layout_height="38dp"
                                android:layout_alignParentBottom="true"

                                android:layout_marginRight="9dp"
                                android:layout_marginBottom="12dp"
                                android:backgroundTint="@color/editButton"


                                android:layout_toLeftOf="@id/s5"
                                android:text="Edit"
                                android:textSize="10sp"
                                android:textStyle="bold" />


                        </RelativeLayout>




                    </androidx.cardview.widget.CardView>





                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_margin="5dp"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        app:cardCornerRadius="15dp"
                        app:cardElevation="100dp"
                        app:cardBackgroundColor="#ffffff"
                        android:layout_height="125dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:padding="5dp"

                            android:layout_height="match_parent">


                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true"
                                app:cardCornerRadius="15dp"
                                android:id="@+id/d1"
                                android:layout_alignParentLeft="true">

                                <ImageView
                                    android:layout_width="125dp"
                                    android:layout_height="125dp"
                                    android:src="@drawable/f3"
                                    android:scaleType="centerCrop"/>



                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/pera"
                                android:textSize="20sp"
                                android:layout_marginLeft="20dp"
                                android:textColor="#000000"
                                android:layout_marginTop="10dp"
                                android:id="@+id/d2"
                                android:layout_toRightOf="@id/d1"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/volume"
                                android:layout_marginTop="10dp"
                                android:id="@+id/d3"
                                android:layout_marginLeft="20dp"
                                android:layout_toRightOf="@id/d1"
                                android:layout_below="@id/d2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/price"
                                android:layout_toRightOf="@id/d1"
                                android:layout_below="@id/d3"
                                android:layout_marginTop="10dp"
                                android:layout_marginLeft="20dp"
                                android:textSize="18sp"
                                android:textStyle="bold"
                                android:id="@+id/d4"
                                android:textColor="#000000"/>


                            <Button
                                android:id="@+id/d5"
                                android:layout_width="74dp"
                                android:layout_height="38dp"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginTop="30dp"

                                android:layout_marginRight="14dp"
                                android:layout_marginBottom="12dp"
                                android:backgroundTint="@color/red"
                                android:text="Delete"
                                android:textSize="10sp"
                                android:textStyle="bold" />

                            <Button
                                android:id="@+id/d6"
                                android:layout_width="63dp"
                                android:layout_height="38dp"
                                android:layout_alignParentBottom="true"
                                android:layout_marginTop="30dp"

                                android:layout_marginRight="9dp"
                                android:layout_marginBottom="12dp"
                                android:backgroundTint="@color/editButton"


                                android:layout_toLeftOf="@id/d5"
                                android:text="Edit"
                                android:textSize="10sp"
                                android:textStyle="bold" />


                        </RelativeLayout>




                    </androidx.cardview.widget.CardView>





                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_margin="5dp"
                    android:layout_height="wrap_content">

                    <androidx.cardview.widget.CardView
                        android:layout_width="match_parent"
                        app:cardCornerRadius="15dp"
                        app:cardElevation="100dp"
                        app:cardBackgroundColor="#ffffff"
                        android:layout_height="125dp">

                        <RelativeLayout
                            android:layout_width="match_parent"
                            android:padding="5dp"

                            android:layout_height="match_parent">


                            <androidx.cardview.widget.CardView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_alignParentTop="true"
                                android:layout_alignParentBottom="true"
                                app:cardCornerRadius="15dp"
                                android:id="@+id/j1"
                                android:layout_alignParentLeft="true">

                                <ImageView
                                    android:layout_width="125dp"
                                    android:layout_height="125dp"
                                    android:src="@drawable/f5"
                                    android:scaleType="centerCrop"/>



                            </androidx.cardview.widget.CardView>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/apple"
                                android:textSize="20sp"
                                android:layout_marginLeft="20dp"
                                android:textColor="#000000"
                                android:layout_marginTop="10dp"
                                android:id="@+id/j2"
                                android:layout_toRightOf="@id/j1"/>

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/volume"
                                android:layout_marginTop="10dp"
                                android:id="@+id/j3"
                                android:layout_marginLeft="20dp"
                                android:layout_toRightOf="@id/j1"
                                android:layout_below="@id/j2" />

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/price"
                                android:layout_toRightOf="@id/j1"
                                android:layout_below="@id/j3"
                                android:layout_marginTop="10dp"
                                android:layout_marginLeft="20dp"
                                android:textSize="18sp"
                                android:textStyle="bold"
                                android:id="@+id/j4"
                                android:textColor="#000000"/>


                            <Button
                                android:id="@+id/j5"
                                android:layout_width="74dp"
                                android:layout_height="38dp"
                                android:layout_alignParentRight="true"
                                android:layout_alignParentBottom="true"
                                android:layout_marginTop="30dp"

                                android:layout_marginRight="14dp"
                                android:layout_marginBottom="12dp"
                                android:backgroundTint="@color/red"
                                android:text="Delete"
                                android:textSize="10sp"
                                android:textStyle="bold" />

                            <Button
                                android:id="@+id/j6"
